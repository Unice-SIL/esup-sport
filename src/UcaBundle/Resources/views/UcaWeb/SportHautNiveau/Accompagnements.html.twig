{% extends "@Uca/Common/Main/Structure.UcaWeb.html.twig" %}
{% import '@Uca/Macros/ImageAsset.html.twig' as ImageAsset %}
{% import '@Uca/Macros/Bootstrap.html.twig' as Bootstrap %}

{% block stylesheets %}
    {{ parent() }}
    {{ ImageAsset.imageFondCssWithEmplacement('SHN - Accompagnement') }}
{% endblock %}

{% block main %}    
    <h1 class="h1-light">{{ 'sportdehautniveau.accompagnements'|trans }}</h1>

    {% if emplacement("SHN - Accompagnement") is not empty %}
        <section class="container container-style bg-white mb-5">
            {{ emplacement("SHN - Accompagnement")|raw }}
        </section>
    {% endif %} 
{% endblock %}
